.c_black {
    color: #000!important;
}
.c_blue {
   color: #04CDFE!important;
}
.video_warpper {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    position: fixed;
    left: 0;
    top: 0;
}
.close_video {
    float: right;
    font-size: 18px;
    display: inline-block;
    margin-right: 4px;
    margin-rop: 4px;
}
.c_white {
    color: #fff !important;
}
.video {
    width: 100%;
}
.video_main {
    width: 100%;
    height: 50%;
    margin-top: 50%;
}
.btn {
    background-color:#cdcdcd;
    padding:10rpx 4rpx;
    margin:0 2px;
    width:60px;
    text-align: center;
}
.border_tb {
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f5f5f5;
}

.row {
    text-align: center;
    margin-top: 10px;
}

.col {
    text-align: center;
}

.margin_auto {
    margin: 0 auto;
}
.exampleUrl {
    width: 50rpx;
    height: 40rpx;
    margin-left: 4rpx;
}
.photoNameWithExamplePhoto {
    display: inline-block;
    width: 185rpx;
    margin-left: 4rpx;
}
.photoNameWithoutExamplePhoto {
    display: inline-block;
    width: 270rpx;
    margin-left: 4rpx;
}
.icon {
   width: 20px;
   height: 26px;
}