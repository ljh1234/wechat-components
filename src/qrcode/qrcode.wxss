@import '../../base/iconfont/iconfont.wxss'
@import '../../base/iconfont/font_fms.wxss'

.qrcode-mask {
  width:100%;
  height:100%;
  position:fixed;
  background-color:#999;
  z-index:9999;
  top:0;
  left:0;
  opacity:0.9;
}

.close-mask {
  position: relative;
  left: 45%;
  top:6px;
}

.qrcode-box{
     text-align:center;
     background-color:#fff;
     height:280px;
     margin-top:100px;
     margin-right:50px;
     margin-left:50px;
}

.canvas {
    width: 200px;
    height: 200px;
    margin: 0 auto
}

.f28{
  font-size:28rpx;
}

.f24{
  font-size:24rpx;
}

.c-g333 {
  color: #333;
}
.c-g666 {
  color: #666;
}

.mt10 {
  margin-top: 10rpx;
}

